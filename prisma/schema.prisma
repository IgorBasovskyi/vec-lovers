generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Icon {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  description String?
  svgIcon     String
  category    String?
  liked       Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model User {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  username        String  @unique
  email           String  @unique
  password        String
}